{
	"name": "@oneai-eu/n8n-nodes-oneai",
	"version": "0.1.0",
	"description": "A n8n community node for interacting with the OneAI API.",
	"keywords": ["n8n-community-node-package"],
	"license": "MIT",
	"homepage": "https://oneai.eu",
	"author": {
		"name": "OneAI",
		"email": "dev@oneai.eu"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/oneai-eu/n8n-nodes-oneai.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"prepublishOnly": "npm run build && npm run lint"
	},
	"files": ["dist"],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/OneAiApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/OneAi/OneAi.node.js"
		],
		"strict": true
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
